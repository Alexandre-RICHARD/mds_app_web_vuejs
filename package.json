{
    "name": "mds_web_app_vuejs",
    "version": "1.0.0",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --history-api-fallback",
        "build": "cross-env NODE_ENV=production webpack",
        "update": "pnpm up",
        "lint": "eslint src/**/*.{js,vue}",
        "lint:fix": "eslint --fix src/**/*.{js,vue}",
        "clean": "rd /s /q .build",
        "clean:all": "rd /s /q node_modules .build & rd /s /q .build & del /q package-lock.json & del /q pnpm-lock.yaml"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Alexandre-RICHARD/mds_app_web_vuejs.git"
    },
    "dependencies": {
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^2.12.0",
        "@types/node": "^22.15.17",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vitest/coverage-v8": "^3.1.3",
        "@vitest/ui": "^3.1.3",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^8.57.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.4.0",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-vitest": "^0.4.1",
        "eslint-plugin-vue": "^10.1.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.5.3",
        "sass": "^1.88.0",
        "sass-loader": "^16.0.5",
        "typescript": "5.5.4",
        "typescript-eslint": "^8.32.0",
        "vite": "^6.3.5",
        "vitest": "^3.1.3",
        "vue-eslint-parser": "^10.1.3",
        "vue-tsc": "^2.2.10"
    }
}
